set format x "%L"
set logscale x 2
set logscale y 
set xlabel "lgN bytes"
set ylabel "GB/Second"
GB=1024*1024*1024
set terminal postscript enhanced color
set output "stats1.eps"
#plot "stats1" u 2:5 i 1 w lp, "" u 2:6 i 1 w lp, "" u 2:($5/$6) i 1 w lp
#plot "stats1" u 2:5 i 2 w lp, "" u 2:6 i 2 w lp, "" u 2:($5/$6) i 2 w lp
#plot "stats1" u 2:5 i 3 w lp, "" u 2:6 i 3 w lp, "" u 2:($5/$6) i 3 w lp
#plot "stats1" u 2:5 i 4 w lp, "" u 2:6 i 4 w lp, "" u 2:($5/$6) i 4 w lp
#plot "stats1" u 2:($2/$5/GB) i 4 w lp, "" u 2:($2/$6/GB) i 4 w lp
plot "stats1" u 2:($2/$5/GB) i 5 w lp t "host to device", "" u 2:($2/$6/GB) i 5 w lp t "device to host"
quit


